H = [0.2 0.1 0.05 0.025 0.0125];
L2 = [0.2074 0.0371 0.0077 0.0019 4.7143e-04];
H1 = [0.1135 0.0328 0.0090 0.0026 8.7547e-04];
Y1 = log(L2);
Y2 = log(H1);
X = log(1./H);
p1 = polyfit(X,Y1,1)
p2 = polyfit(X,Y2,1)
x = X(1):(X(5)-X(1))/100:X(5);
y1 = p1(1)x+p1(2);
y1 = p2(1)x+p2(2);
figure(1)
hold on
plot(X,Y1,'--*')
plot(X,Y2,'--*')
plot(,Y1,'--*')
plot(X,Y2,'--*')